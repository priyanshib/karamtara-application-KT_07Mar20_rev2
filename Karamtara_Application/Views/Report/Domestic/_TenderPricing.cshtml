    function SetTotalGrossWt() {
        var totalWt = 0,totalNtWt = 0;
        lineGrWt = [];

        $(revParentDiv).find(lineArray).each(function (index, item) {
            //
            var lineItems = $(revParentDiv).find("td[name=" + item.LineName + "]");
