    function SetTotalSalesCostExWorks() {
        var maxValue = 0,totalSalesCost = 0,totalExWorks = 0;
        var lineElements = $(revParentDiv).find('.TenderDtlTable table').find('td.totSalesCost').not(".excludeItem");
        ////
        $(revParentDiv).find(lineElements).each(function (index, ele) {
            maxValue = 0;
            var lineItems = $(revParentDiv).find(ele).parent().find('td.salesCost');
            $(revParentDiv).find(lineItems).each(function (count, element) {
                var value = parseFloat($(revParentDiv).find(element).text())
                if (value != "" && !isNaN(value)) {
                    maxValue = maxValue < value ? value : maxValue;
                }
            });
            $(revParentDiv).find(ele).text(maxValue.toFixed(3));
            totalSalesCost = totalSalesCost + maxValue;

            maxValue = 0;

            lineItems = $(revParentDiv).find(ele).parent().find('td.sCostExWorks');
            $(revParentDiv).find(lineItems).each(function (count, element) {
                var value = parseFloat($(revParentDiv).find(element).text())
                if (value != "" && !isNaN(value)) {
                    maxValue = maxValue < value ? value : maxValue;
                }
            });
            $(revParentDiv).find(ele).parent().find(".totExWorks").text(maxValue.toFixed(3));
            totalExWorks = totalExWorks + maxValue;
        });

        $(revParentDiv).find(".totalWtRow").find(".totSalesCost").text(totalSalesCost.toFixed(3));
        $(revParentDiv).find(".totalWtRow").find(".totExWorks").text(totalExWorks.toFixed(3));
    }

