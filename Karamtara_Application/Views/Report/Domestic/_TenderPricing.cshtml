    function SetLineFreight() {
        var freight = 0,totalNtWt = 0,sum=0,value = 0;
        var lineElements = $(revParentDiv).find('.TenderDtlTable table').find('td.lineFreight').not(".excludeItem");
        //
        $(revParentDiv).find(lineElements).each(function (index, ele) {
            freight = $(revParentDiv).find(ele).attr("freight");
            totalNtWt = $(revParentDiv).find(ele).closest("tr").find(".totNtWt").text();

            if (freight == "" || isNaN(freight))
                freight = 0;
            if (totalNtWt == "" || isNaN(totalNtWt))
                totalNtWt = 0;

            value = Number(freight * (totalNtWt/ 1000)).toFixed(3);

            sum = sum + Number(value);

            $(revParentDiv).find(ele).text(value);
        });

        $(revParentDiv).find(".totalWtRow").find(".lineFreight").text(sum.toFixed(3));
    }

