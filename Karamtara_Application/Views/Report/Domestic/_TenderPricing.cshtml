    function pushDetails(element) {
        var obj = {};
        var lineQtyId = $(revParentDiv).find(element).attr("lineqtyid");
        var index = tenderLineDetails.findIndex(x => x.LineQtyId == lineQtyId);
        if (index != -1) {
            tenderLineDetails[index].Quantity = $(revParentDiv).find(element).val();
        }
        else {
            var customId = $(revParentDiv).find(element).attr("lineqtyid");

            if (customId == "0") {
                customId = randomId();
            }
            $(revParentDiv).find(element).attr("lineqtyid", customId)
            obj.LineQtyId = customId;
            obj.LineId = $(revParentDiv).find(element).attr("lineId");;
            obj.Quantity = $(revParentDiv).find(element).val();
            obj.TenderId = $(revParentDiv).find("#tndId").val();
            obj.TenderRevNo = $(revParentDiv).find("#tndRevId").val();
            obj.BomId = $(revParentDiv).find("#bomId").val();
            obj.RevisionNo = $(revParentDiv).find("#revisionId").val();
            obj.ProductId = $(revParentDiv).find(element).attr('productid');
            obj.Type = $(revParentDiv).find(element).attr('typeid');
            obj.ProductGroupId = $(revParentDiv).find(element).attr('productgroupid');
            obj.AssemblyId = $(revParentDiv).find(element).attr('assemblyid');
            obj.SubAssemblyId = $(revParentDiv).find(element).attr('subassemblyid');
            obj.ComponentId = $(revParentDiv).find(element).attr('componentid');
            tenderLineDetails.push(obj);
            obj = {};
        }

    }

    function setTotalLineQuantity() {

        var lineElements = $(revParentDiv).find('.TenderDtlTable table').find('td.qtyWtTotal').not(".excludeWt");

        $(revParentDiv).find(lineElements).each(function (index, ele) {
            var total = 0,lineId  = 0;
            var lineItems = $(revParentDiv).find(ele).parent().find('td.qtyWt');
            $(revParentDiv).find(lineItems).each(function (count, element) {
                var value = parseFloat($(revParentDiv).find(element).find("input").val());
                lineId = $(revParentDiv).find(element).find("input").attr("lineid");
                var unitCost = $(revParentDiv).find(element).parent().find('.calcUnitCost').text();
                if (value != "" && !isNaN(value)) {
                    total = total + parseFloat(value);
                    if (unitCost != "" && !isNaN(unitCost)) {
                        $(revParentDiv).find(element).parent().find('td.lineUnitCost[lineid="' + lineId + '"]').text((value * unitCost).toFixed(3));
                    }
                }

            });
            $(revParentDiv).find(ele).text(total);

            var totalGrWt = $(revParentDiv).find(ele).parent().find('td.totGrWt');
            var UnitGrWt = $(revParentDiv).find(ele).parent().find('td.uniGrWt').text();
            var totalNtWt = $(revParentDiv).find(ele).parent().find('td.totNtWt');
            var UnitNtWt = $(revParentDiv).find(ele).parent().find('td.uniNtWt').text();

            if (UnitGrWt != "" && !isNaN(UnitGrWt))
                $(revParentDiv).find(totalGrWt).text((total * parseFloat(UnitGrWt)).toFixed(3));
            if (UnitNtWt != "" && !isNaN(UnitNtWt))
                $(revParentDiv).find(totalNtWt).text((total * parseFloat(UnitNtWt)).toFixed(3));
        });

        lineElements = $(revParentDiv).find('.TenderDtlTable table').find('td.calcUnitCost');
        var unitCost = 0;
        $(revParentDiv).find('.TenderDtlTable table').find('td.calcUnitCost').each(function (index, item) {
            unitCost += Number($(revParentDiv).find(item).text());
        })
        $(revParentDiv).find("#txtUnitCost").val(unitCost.toFixed(3));
        SetTotalGrossWt();
        setLineUnitCost();
    }

    function randomId() {
        var S4 = function () {
            return (((1 + Math.random()) * 0x10000) | 0).toString(16).substring(1);
        };
        return (S4() + S4() + "-" + S4() + "-" + S4() + "-" + S4() + "-" + S4() + S4() + S4());
    }

