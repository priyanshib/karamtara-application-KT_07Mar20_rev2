                var unitGrWt = $(revParentDiv).find(element).parent().find(".uniGrWt").text();
                var unitNtWt = $(revParentDiv).find(element).parent().find(".uniNtWt").text();
                if (lineQty == "" && isNaN(lineQty))
                    lineQty = 0;

                if (unitNtWt == "" && isNaN(unitNtWt))
                    unitNtWt = 0;

                if (unitGrWt == "" && isNaN(unitGrWt))
                    unitGrWt = 0;

                totalNtWt = totalNtWt + (lineQty * unitNtWt);

                totalWt = totalWt + (lineQty * unitGrWt);
            });

            //$(revParentDiv).find(".totalWtRow").find("." + item.LineName).text(totalWt.toFixed(3));
            lineGrWt.push({ LineId: item.LineId, TndId: $(revParentDiv).find("#tndId").val(), TndRevNo: $(revParentDiv).find("#tndRevId").val(), GrWt: totalWt.toFixed(3), NtWt: totalNtWt.toFixed(3)});
            totalWt = 0;
            totalNtWt = 0;

        });
    }

